{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Gediminas Mila≈°ius",
    "email": "gediminasmilasius@gmail.com",
    "url": "http://gedasfx.github.io/"
  },
  "main": "firebase-server.js",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "export": "next export",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint \"src/**/*.+(js|jsx|ts|tsx)\"",
    "lint:prod": "yarn run lint --max-warnings 0",
    "format": "prettier \"src/**/*.+(js|jsx|ts|tsx)\" --write",
    "deploy": "next build && cross-env NODE_ENV=production firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "next": "10.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "firebase-functions": "^3.13.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.0.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}